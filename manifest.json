{
  "manifest_version": 2,

  "name": "Play on Spotify",
  "description": "This extension plays music in spotify if you highlight any song name and right-click",
  "version": "1.0",

  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [{
      "matches": [
          "<all_urls>"
      ],
      "js": ["content_script.js"]
  }],
  "background": {
      "scripts": [
          "lib/jquery.min.js",
          "background.js"
      ]
  }
}